{
  "manifest_version": 2,
  "version": "0.0.1",
  "name": "Drawler",
  "description": "Draw box around elements",
  "permissions": [
    "tabs",
    "contextMenus",
    "notifications",
    "activeTab",
    "declarativeContent",
    "storage",
    "<all_urls>"
  ],
  "icons": {
    "48": "icons/4848.png"
  },
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icons/4848.png"
    }
  },
  "options_page": "options.html"
}
