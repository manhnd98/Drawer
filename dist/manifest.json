{
  "manifest_version": 2,
  "version": "0.0.1",
  "name": "Drawler",
  "description": "Draw box around elements",
  "permissions": [
    "tabs",
    "contextMenus",
    "notifications",
    "activeTab",
    "declarativeContent",
    "storage",
    "<all_urls>"
  ],
  "icons": {
    "16": "icons/get_started16.png",
    "32": "icons/get_started32.png",
    "48": "icons/get_started48.png",
    "128": "icons/get_started128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ]
  }
}
